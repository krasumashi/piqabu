{
  "expo": {
    "name": "Piqabu",
    "slug": "piqabu",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "userInterfaceStyle": "dark",
    "newArchEnabled": true,
    "splash": {
      "image": "./assets/splash-icon.png",
      "resizeMode": "contain",
      "backgroundColor": "#0F1114"
    },
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.krasumashi.piqabu",
      "infoPlist": {
        "ITSAppUsesNonExemptEncryption": false,
        "NSCameraUsageDescription": "Piqabu uses the camera for the Live Glass feature",
        "NSMicrophoneUsageDescription": "Piqabu uses the microphone for the Whisper feature"
      }
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#0F1114"
      },
      "edgeToEdgeEnabled": true,
      "package": "com.krasumashi.piqabu"
    },
    "web": {
      "favicon": "./assets/favicon.png",
      "bundler": "metro",
      "output": "single"
    },
    "extra": {
      "eas": {
        "projectId": "eb9a4e25-cfdd-4c53-b25c-d5e52973595c"
      }
    },
    "plugins": [
      "expo-router",
      "expo-secure-store",
      "expo-font"
    ]
  }
}
